            steps {
                echo "Init stage"
                git --version
            }
        }
        stage('build') {
            steps {
                echo "Buid stage"
                git --version
            }
        }
        stage('deploy') {
            steps {
                echo "Deploy stage"
                git --version
            }
        }

    }
}